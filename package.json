{
  "name": "Orthodemo",
  "version": "1.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pack": "npx electron-builder --dir",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "electron": "electron .",
    "electron-build": "ng build && electron .",
    "electron-package-win": "ng build && electron-packager . --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite",
    "electron-package-mac": "ng build --prod && electron-packager . --platform=darwin --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.5",
    "@angular/common": "11.2.5",
    "@angular/compiler": "11.2.5",
    "@angular/core": "11.2.5",
    "@angular/forms": "11.2.5",
    "@angular/localize": "^11.2.5",
    "@angular/platform-browser": "11.2.5",
    "@angular/platform-browser-dynamic": "11.2.5",
    "@angular/router": "11.2.5",
    "@ng-bootstrap/ng-bootstrap": "7.0.0",
    "@ngui/map": "0.30.3",
    "@types/googlemaps": "3.39.14",
    "arrive": "2.4.1",
    "bootstrap": "4.5.2",
    "bootstrap-notify": "3.1.3",
    "chart.js": "2.9.3",
    "chartist": "0.11.4",
    "core-js": "3.6.5",
    "electron-packager": "^15.2.0",
    "express": "4.17.1",
    "jquery": "3.5.1",
    "moment": "2.29.1",
    "ngx-electron": "^2.2.0",
    "ngx-toastr": "13.1.0",
    "rxjs": "6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.4",
    "@angular/cli": "^11.2.4",
    "@angular/compiler-cli": "11.2.5",
    "@types/bootstrap": "5.0.0",
    "@types/chartist": "0.11.0",
    "@types/jasmine": "~3.6.0",
    "@types/jquery": "3.5.2",
    "@types/node": "14.11.5",
    "codelyzer": "^6.0.0",
    "electron": "^12.0.1",
    "electron-builder": "^22.9.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "7.0.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.3"
  }
}
